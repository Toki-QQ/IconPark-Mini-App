{
  "name": "@iconpark-mini-app",
  "version": "1.0.0",
  "description": "An iconpark provider for taro.js and wechat miniprogram.",
  "main": "packages",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "build:cleanTaro": "rimraf ./packages/taro_vue/lib",
    "build:cleanWeapp": "rimraf ./packages/weapp/lib",
    "build:clean": "pnpm build:cleanTaro && pnpm build:cleanWeapp",
    "build:projects": "gulp",
    "build": "pnpm run build:clean && pnpm build:projects",
    "commit": "git add . && git cz"
  },
  "keywords": [
    "iconpark",
    "IconPark",
    "taro",
    "taro.js",
    "wechat",
    "miniprogram"
  ],
  "author": "Toki",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/runtime": "^7.7.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/gulp": "^4.0.10",
    "@types/merge2": "^1.4.0",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "alias-for-vue3": "0.0.1-security",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "husky": "^8.0.3",
    "merge2": "^1.4.1",
    "prettier": "2.6.2",
    "rimraf": "^4.0.4",
    "rollup": "^3.20.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.0"
  },
  "dependencies": {
    "@icon-park/svg": "1.4.2",
    "svg64": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  }
}